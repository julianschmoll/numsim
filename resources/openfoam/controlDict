FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

adjustTimeStep  yes;

maxCo           2; // Allows a Courant number higher than 1

maxDeltaT       $maximumDt;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         $endTime;

deltaT          $maximumDt;

writeControl    runTime;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


