FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);


boundaryField
{
    top
    {
        $pointTop
    }
    bottom
    {
        $pointBottom
    }
    left
    {
        $pointLeft
    }
    right
    {
        $pointRight
    }
    frontAndBack
    {
        type            empty;
    }
}

